<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AttachButton">
        <attr name="attachText" format="string" />
        <attr name="attachIcon" format="reference" />
        <attr name="attachColor" format="color" />
    </declare-styleable>

    <attr name="editable" format="boolean" />
    <attr name="labelText" format="string" />
    <attr name="hintText" format="string" />
    <attr name="font" format="string" />

    <declare-styleable name="EntryTextBlock">
        <attr name="labelText" />
        <attr name="hintText" />
        <attr name="editable" />
    </declare-styleable>

    <declare-styleable name="EntryToggleBlock">
        <attr name="labelText" />
    </declare-styleable>

    <declare-styleable name="EntrySelectBlock">
        <attr name="labelText" />
        <attr name="hintText" />
        <attr name="editable" />
        <attr name="selectIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="EntryDateBlock">
        <attr name="labelText" />
        <attr name="editable" />
    </declare-styleable>

    <declare-styleable name="EntrySpinnerBlock">
        <attr name="labelText" />
    </declare-styleable>

    <declare-styleable name="FormView">
        <attr name="layout" />
        <attr name="labelText" />
    </declare-styleable>

    <declare-styleable name="FontTextView">
        <attr name="font" />
    </declare-styleable>

    <declare-styleable name="FontButton">
        <attr name="font" />
    </declare-styleable>

    <declare-styleable name="FontEditText">
        <attr name="font" />
    </declare-styleable>

    <declare-styleable name="IOSToggle">
        <attr name="color" />
        <attr name="leftText" format="string" />
        <attr name="rightText" format="string" />
    </declare-styleable>
</resources>